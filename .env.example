PORT=3009

# Rust+ connection
RUST_ADDRESS=127.0.0.1
RUST_PORT=28082
# Steam64 ID of the player paired to the server
RUST_PLAYER_ID=00000000000000000
# Player token from pairing (can be negative)
RUST_PLAYER_TOKEN=0

# Optional: use Facepunch proxy (wss)
RUST_USE_FACEPUNCH_PROXY=false

# Optional: request timeout ms
RUST_TIMEOUT_MS=10000

# Optional cache TTLs (ms)
# Reduce Rust+ token/rate usage by caching responses.
INFO_TTL_MS=10000
VENDING_TTL_MS=5000
MAP_TTL_MS=300000

# Note: The output from `npx @liamcottle/rustplus.js fcm-register` (fcm/expo/auth tokens)
# is NOT used by this app. Instead, run `npx @liamcottle/rustplus.js fcm-listen` and
# press Pair in-game, then copy ip/port/playerId/playerToken here.
